{
  "name": "vacuum-sol",
  "version": "1.0.5",
  "description": "Vacuum - Suck up forgotten rent from Solana accounts",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "vacuum": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "bot": "node dist/cli.js bot",
    "bot:dev": "tsx src/cli.ts bot",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "simulate": "tsx scripts/simulate.ts"
  },
  "keywords": [
    "solana",
    "rent",
    "reclaim",
    "automation",
    "cli",
    "bot",
    "telegram",
    "web3",
    "token-accounts"
  ],
  "author": "Don-Vicks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Don-Vicks/vaccum.git"
  },
  "homepage": "https://vaccum-bot.vercel.app",
  "bugs": {
    "url": "https://github.com/Don-Vicks/vaccum/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "cors": "^2.8.6",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "ora": "^8.1.1",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
